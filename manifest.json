{
  "name": "Medium Unblocker",
  "version": "0.1",
  "manifest_version": 3,
  "action": {
    "default_title": "Click to unblock"
  },
  "icons": {
    "128": "images/disabled_icon.png"
  },
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  }
}
